<?xml version="1.0" encoding="UTF-8"?>
<project>

    <description>Module that defines the default client bus configuration (with configuration variables and default values).</description>

    <exported-packages>
        <source-packages/>
        <resource-packages/>
    </exported-packages>

    <dependencies>
        <used-by-source-modules/>
    </dependencies>

    <embed-resources>
        <resource>one/modality/base/client/busconfig/ClientBusOptions.default.json</resource>
    </embed-resources>

    <providers>
        <provider interface="dev.webfx.stack.conf.spi.ConfigurationSupplier">one.modality.base.client.busconfig.ModalityClientBusOptionsConfigurationSupplier</provider>
    </providers>

</project>