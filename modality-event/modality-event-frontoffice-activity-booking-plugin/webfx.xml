<?xml version="1.0" encoding="UTF-8"?>
<project>

    <description>The front-office booking activity.</description>

    <exported-packages>
        <source-packages/>
    </exported-packages>

    <dependencies>
        <used-by-source-modules/>
    </dependencies>

    <providers>
        <provider interface="dev.webfx.stack.routing.uirouter.UiRoute">one.modality.event.frontoffice.activities.booking.BookingUiRoute</provider>
        <provider interface="dev.webfx.stack.routing.uirouter.operations.RouteRequestEmitter">one.modality.event.frontoffice.activities.booking.RouteToBookingRequestEmitter</provider>
    </providers>

    <web-callbacks> <!-- not yet considered by WebFX CLI but will be in the future to automatically generate WebToJavaCallbacks super source in final executable GWT module -->
        <callback-class name="one.modality.event.frontoffice.activities.booking.views.DynamicMapView">
            <callback-method name="onGoogleMapLoaded"/>
            <callback-method name="onMarkerClicked">
                <callback-argument class="one.modality.event.frontoffice.activities.booking.views.MapMarker"/>
            </callback-method>
        </callback-class>
    </web-callbacks>

</project>